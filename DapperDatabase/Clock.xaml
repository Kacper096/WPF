<UserControl x:Class="DapperDatabase.Clock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DapperDatabase"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100" Name="ClockDate">
    <Label Name="Date" Style="{StaticResource StyleClock}">
        <StackPanel>
            <Label Name="calendar" FontWeight="Bold" FontSize="20"/>
            <Label Name="timer" />
        </StackPanel>
    </Label>
</UserControl>
